{
  "name": "pgAdmin",
  "version": "4.0.5-dev",
  "description": "pgAdmin is the most popular and feature rich Open Source administration and development platform for PostgreSQL, the most advanced Open Source database in the world.",
  "main": "src/index.js",
  "repository": "git.postgresql.org",
  "author": "pgAdmin Hackers",
  "license": "PostgreSQL",
  "scripts": {
    "start": "cross-env NODE_ENV=production yarn run electron .",
    "start:dev": "yarn run electron .",
    "package": "",
    "make": "",
    "publish": "",
    "dist:deb": "",
    "dist:rpm": "",
    "dist:darwin": "electron-builder --mac --x64",
    "dist:windows": "electron-builder --win --x64",
    "linter": "yarn eslint --no-eslintrc -c .eslintrc.js --ext .js .",
    "copy-folder:nix": "cp -r ../web web && find web | grep .pyc$ | xargs -I '{}' rm '{}'",
    "build:nix": "yarn copy-folder:nix && yarn bundle-app-js",
    "test": "yarn lint && cross-env ENV=TEST yarn mocha"
  },
  "keywords": [
    "pgadmin",
    "pgadmin4",
    "postgres",
    "sqleditor",
    "sql",
    "query",
    "database"
  ],
  "build": {
    "appId": "com.postgres.pgAdmin",
    "asar": false,
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "zip"
      ],
      "icon": "assets/icons/pgAdmin4.icns"
    },
    "nsis": {
      "perMachine": true
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "cross-env": "^5.1.5",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.3.1",
    "logform": "^1.6.0",
    "popper.js": "^1.15.0",
    "winston": "^3.0.0-rc5"
  },
  "devDependencies": {
    "electron": "^6.0.1",
    "electron-builder": "^21.2.0"
  }
}
